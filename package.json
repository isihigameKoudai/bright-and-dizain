{
  "name": "bright-and-dezain",
  "version": "1.0.0",
  "description": "Bright&dezain web site",
  "author": "koudaiishigame <koudaiishigame@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt --spa",
    "build": "nuxt build --es-module-specifier-resolution=node",
    "build:storybook": "build-storybook -s story-static",
    "build:analyze": "nuxt build --analyze",
    "test:unit": "jest",
    "start": "nuxt start",
    "generate": "nuxt generate --es-module-specifier-resolution=node",
    "dev:gen": "cd dist && python -m SimpleHTTPServer",
    "dev:storybook": "start-storybook -p 9001 -c .storybook -s src/static",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.30.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@types/webpack": "^5.28.0",
    "babel-core": "^6.26.3",
    "immer": "^9.0.6",
    "jquery": "^3.6.0",
    "mime": "^3.0.0",
    "nuxt": "^2.15.8"
  },
  "devDependencies": {
    "@nuxt/config": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@storybook/vue": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-controls": "^6.3.12",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.3.12",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/test-utils": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-vue": "^2.0.2",
    "clone": "^2.1.2",
    "css-loader": "^6.5.1",
    "debug": "^4.3.2",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^8.1.1",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "node-sass": "^6.0.1",
    "prettier": "^2.4.1",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "tar": "^6.1.11",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.1",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.8",
    "vue-svg-loader": "^0.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$",
    "testURL": "http://localhost/"
  }
}
