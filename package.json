    "@types/webpack": "^4.41.0",
    "babel-core": "^6.26.3",
    "jquery": "^3.4.1",
    "mime": "^2.4.4",
    "nuxt": "^2.10.2",
    "vue-awesome-swiper": "^3.1.3"
  },
  "devDependencies": {
    "@nuxt/config": "^2.10.2",
    "@nuxt/typescript-build": "^0.3.4",
    "@storybook/vue": "^5.2.6",
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@vue/test-utils": "^1.0.0-beta.30",
    "ajv": "^6.10.2",
    "babel-eslint": "^10.0.3",
    "babel-preset-vue": "^2.0.2",
