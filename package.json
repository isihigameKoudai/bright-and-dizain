{
  "name": "bright-and-dezain",
  "version": "1.0.0",
  "description": "Bright&dezain web site",
  "author": "koudaiishigame <koudaiishigame@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts --spa",
    "build": "nuxt-ts build --es-module-specifier-resolution=node",
    "build:storybook": "build-storybook -s story-static",
    "build:analyze": "nuxt-ts build --analyze",
    "test:unit": "jest",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate --es-module-specifier-resolution=node",
    "dev:gen": "cd dist && python -m SimpleHTTPServer",
    "dev:storybook": "start-storybook -p 9001 -c .storybook -s src/static",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/composition-api": "^0.20.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@types/webpack": "^4.41.26",
    "babel-core": "^6.26.3",
    "immer": "^8.0.1",
    "jquery": "^3.5.1",
    "mime": "^2.5.2",
    "nuxt": "^2.15.1"
  },
  "devDependencies": {
    "@nuxt/config": "^2.15.1",
    "@nuxt/typescript-build": "^2.0.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^5.3.19",
    "@storybook/vue": "^6.1.18",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-vue": "^2.0.2",
    "clone": "^2.1.2",
    "css-loader": "^5.0.2",
    "debug": "^4.3.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^7.6.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "tar": "^6.1.0",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "url-loader": "^4.1.1",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.6",
    "vue-svg-loader": "^0.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$",
    "testURL": "http://localhost/"
  }
}
