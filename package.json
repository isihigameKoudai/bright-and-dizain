{
  "name": "bright-and-dezain",
  "version": "1.0.0",
  "description": "Bright&dezain web site",
  "author": "koudaiishigame <koudaiishigame@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts --spa",
    "build": "nuxt-ts build --es-module-specifier-resolution=node",
    "build:storybook": "build-storybook -s story-static",
    "build:analyze": "nuxt-ts build --analyze",
    "test:unit": "jest",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate --es-module-specifier-resolution=node",
    "dev:gen": "cd dist && python -m SimpleHTTPServer",
    "dev:storybook": "start-storybook -p 9001 -c .storybook -s src/static",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.1",
    "@nuxtjs/axios": "^5.9.6",
    "@nuxtjs/composition-api": "^0.18.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/pwa": "^3.0.2",
    "@types/webpack": "^4.41.8",
    "babel-core": "^6.26.3",
    "jquery": "^3.5.1",
    "mime": "^2.4.4",
    "nuxt": "^2.14.5"
  },
  "devDependencies": {
    "@nuxt/config": "^2.12.1",
    "@nuxt/typescript-build": "^0.6.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/vue": "^5.3.17",
    "@types/jest": "^25.1.4",
    "@types/node": "^14.0.26",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "ajv": "^6.12.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-vue": "^2.0.2",
    "clone": "^2.1.2",
    "css-loader": "^3.4.2",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^6.2.2",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "husky": "^4.3.7",
    "jest": "^25.2.3",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "tar": "^6.0.1",
    "ts-jest": "^26.3.0",
    "ts-loader": "^6.2.2",
    "url-loader": "^4.0.0",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.1",
    "vue-svg-loader": "^0.16.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?|ts?)$",
    "testURL": "http://localhost/"
  }
}
